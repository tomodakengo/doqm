{
  "$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
  "organizeImports": {
    "enabled": true
  },
  "linter": {
    "enabled": true,
    "rules": {
      "recommended": true,
      "a11y": {
        "useKeyWithClickEvents": "off",
        "useButtonType": "off"
      },
      "complexity": {
        "noForEach": "off",
        "useOptionalChain": "off"
      },
      "style": {
        "noNonNullAssertion": "off"
      }
    }
  }
}
